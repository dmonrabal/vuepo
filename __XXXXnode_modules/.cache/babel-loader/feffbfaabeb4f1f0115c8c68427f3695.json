 catch (e) {\n      console.log('[ERROR] - logIn: ' + e.message);\n    }\n  },\n  logOut({ commit, state }) {\n    commit('setUser', '');\n  },\n};\n\nexport default { namespaced: true, state, getters, mutations, actions };\n"]}]}