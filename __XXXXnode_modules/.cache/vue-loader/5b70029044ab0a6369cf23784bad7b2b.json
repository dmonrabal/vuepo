card class=\"mx-auto\" max-width=\"300\">\n          <v-img\n            height=\"200\"\n            src=\"https://cdn.vuetifyjs.com/images/cards/cooking.png\"\n          ></v-img>\n          <v-card-title>\n            <h2 class=\"text-h6\">{{ project.name }}</h2>\n            <v-spacer></v-spacer>\n          </v-card-title>\n\n          <v-card-text> {{ project.description }} </v-card-text>\n\n          <v-divider class=\"mx-4\"></v-divider>\n\n          <v-card-text>\n            <span class=\"subheading\">Select size</span>\n          </v-card-text>\n\n          <v-card-actions>\n            <v-btn block class=\"white--text mb-2\" color=\"success accent-4\">\n              Select project\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport { mapState, mapActions } from 'vuex';\nimport Loading from '@/components/Loading';\nimport Title from '@/components/Title';\n\nexport default {\n  name: 'Home',\n  cargando: false,\n  props: ['loading', 'title', 'path'],\n  components: {\n    Loading,\n    Title\n  },\n  computed: {\n    ...mapState(['projects', 'users']),\n  },\n  methods: {\n    ...mapActions(['getProjects']),\n\n    logInreadProjects() {\n      this.cargando = true;\n      this.$store.dispatch('projects/getProjects', this.users.user.token);\n      //this.$store.dispatch('projects/getStatsProject', this.users.user.token)\n      this.cargando = false;\n    },\n  },\n  created() {\n    this.logInreadProjects();\n  },\n};\n</script>\n\n<style scoped>\n.projectlist {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  margin: 0;\n}\n</style>\n"]}]}